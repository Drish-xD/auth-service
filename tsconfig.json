{
	"compilerOptions": {
		"target": "ESNext",
		"jsx": "react-jsx",
		"jsxImportSource": "hono/jsx",
		"baseUrl": "./",
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"paths": {
			"@/*": ["./src/*"]
		},
		"types": ["node"],
		"strict": true
	},
	"tsc-alias": {
		"resolveFullPaths": true
	}
}
